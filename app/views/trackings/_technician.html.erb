<div class="panel panel-default">
  <div class="panel-heading">
    Tracking: <%= technician.name %>
  </div>
  <div class="panel-body">
    <table class="table">
      <tbody>
      <tr>
        <td>Name</td>
        <td><%= technician.name %></td>
      </tr>
      <tr>
        <td>Department</td>
        <td><%= technician.department.name if technician.department %></td>
      </tr>

      <tr>
        <td>Nearest Zone</td>
        <td><%= technician.zone.title if technician.zone %></td>
      </tr>

      <tr>
        <td>Site</td>
        <td><%= technician.site.title if technician.site %></td>
      </tr>

      <tr>
        <td>Total Assigned WO</td>
        <td> 10</td>
      </tr>

      <tr>
        <td>Completed WO</td>
        <td> 5</td>
      </tr>

      <tr>
        <td>In Progress WO</td>
        <td> 3</td>
      </tr>

      <tr>
        <td>Failed To Complete</td>
        <td> 2</td>
      </tr>
      </tbody>
    </table>
  </div>
</div>